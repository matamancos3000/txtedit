from pywinauto import Desktop

try:
    dlg = Desktop(backend="win32").window(title_re=".*MX CWX Desktop.*")
    print("Ventana encontrada:", dlg.window_text())

    print("Minimizando ventana...")
    dlg.minimize()

    print("Restaurando ventana...")
    dlg.restore()

    print("Listo ðŸ˜Š")

except Exception as e:
    print("Error:", e)
