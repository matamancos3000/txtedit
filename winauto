from pywinauto import Desktop

try:
    dlg = Desktop(backend="win32").window(title_re=".*MX CWX.*")
    print("Ventana encontrada:", dlg.window_text())

    print("Intentando restaurar...")
    dlg.restore()

    print("Intentando minimizar...")
    dlg.minimize()

    print("Hecho.")
except Exception as e:
    print("Error:", e)
